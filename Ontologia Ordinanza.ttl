<http://example.com/ordinanza> rdf:type owl:Ontology ;
                                dc:creator "Cocca Simone" ;
                                dc:description "Questa ontologia descrive la struttura di un'ordinanza comunale della viabilità"@it ;
                                dc:title "Ontologia per le Ordinanze della viabilità"@it ;
                                rdfs:comment "Concept related to ordinanza."@en ,
                                             "Concetto relativo a ordinanza."@it ;
                                rdfs:label "ordinanza"@it .

#################################################################
#    Annotation properties
#################################################################

###  http://cef.uv.es/lodroadtran18/def/transporte/dtx_srti#hasOverallStartTime
dtx:hasOverallStartTime rdf:type owl:AnnotationProperty ;
                        rdfs:comment "Indica l'orario di inizio" ;
                        rdfs:label "Has start Time" ;
                        rdfs:domain dtx:PublicEvent .


###  http://purl.org/dc/elements/1.1/#creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/#description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/#title
dc:title rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#closeMatch
skos:closeMatch rdf:type owl:AnnotationProperty ;
                rdfs:subPropertyOf rdfs:seeAlso .


#################################################################
#    Object Properties
#################################################################

###  http://cef.uv.es/lodroadtran18/def/transporte/dtx_srti#directionAtPoint
dtx:directionAtPoint rdf:type owl:ObjectProperty ;
                     rdfs:domain ex:DirezioneObbligatoria ,
                                 ex:IstituzioneSensoUnico ;
                     rdfs:range dtx:DirectionEnum ;
                     rdfs:comment "Indica la direzione di marcia prevista per un determinato ordine."@it ,
                                  "Specifies the direction of travel for a given order."@en ;
                     rdfs:label "ha direzione"@it .


###  http://cef.uv.es/lodroadtran18/def/transporte/dtx_srti#hasPublicEventType
dtx:hasPublicEventType rdf:type owl:ObjectProperty ;
                       rdfs:domain dtx:PublicEvent ;
                       rdfs:range dtx:PublicEventTypeEnum .


###  http://example.com/ordinanza#eccetto_categoria_veicoli
ex:eccetto_categoria_veicoli rdf:type owl:ObjectProperty ;
                             rdfs:domain ex:CapolineaBusTaxi ,
                                         ex:DirezioneObbligatoria ,
                                         ex:DivietoAccessoVeicolare ,
                                         ex:DivietoSostaRimozione ,
                                         ex:DivietoTransitoVeicolare ,
                                         ex:IstituzioneCorsiaRiservata ,
                                         ex:SensoVietato ,
                                         ex:SospensioneCorsiaRiservata ,
                                         ex:SostaConsentita ,
                                         ex:SostaRiservata ,
                                         ex:StradaChiusaTotale ;
                             rdfs:range ex:Veicoli ;
                             rdfs:comment "Concept related to eccetto categoria veicoli."@en ,
                                          "Concetto relativo a eccetto categoria veicoli."@it ;
                             rdfs:label "eccetto veicoli"@it .


###  http://example.com/ordinanza#firmata_da
ex:firmata_da rdf:type owl:ObjectProperty ;
              rdfs:domain ex:Ordinanza ;
              rdfs:range foaf:person ;
              rdfs:comment "Defines the person who signed the ordinance."@en ,
                           "Definisce il nome della persona che ha firmato l'ordinanza" ;
              rdfs:label "signed by"@en ,
                         "Firmata da" .


###  http://example.com/ordinanza#ha_categoria_veicoli
ex:ha_categoria_veicoli rdf:type owl:ObjectProperty ;
                        rdfs:domain ex:CapolineaBusTaxi ,
                                    ex:DirezioneObbligatoria ,
                                    ex:DivietoAccessoVeicolare ,
                                    ex:DivietoSostaRimozione ,
                                    ex:DivietoTransitoVeicolare ,
                                    ex:IstituzioneCorsiaRiservata ,
                                    ex:SensoVietato ,
                                    ex:SospensioneCorsiaRiservata ,
                                    ex:SostaConsentita ,
                                    ex:SostaRiservata ,
                                    ex:StradaChiusaTotale ;
                        rdfs:range ex:Veicoli ;
                        rdfs:comment "Concept related to ha categoria veicoli."@en ,
                                     "Concetto relativo a ha categoria veicoli."@it ;
                        rdfs:label "ha categoria veicoli"@it .


###  http://example.com/ordinanza#ha_luogo
ex:ha_luogo rdf:type owl:ObjectProperty ;
            rdfs:domain ex:Ordine ;
            rdfs:range ex:Luogo_Ordine ;
            rdfs:comment "Indica il luogo in cui si applica l'ordine viabilistico."@it ,
                         "Indicates the location to which the traffic order applies."@en ;
            rdfs:label "ha luogo"@en ,
                       "ha luogo"@it ,
                       "ha_luogo"@en .


###  http://example.com/ordinanza#stabilisce
ex:stabilisce rdf:type owl:ObjectProperty ;
              rdfs:domain ex:Ordinanza ;
              rdfs:range ex:Ordine ;
              rdfs:comment "Collega un'ordinanza al tipo di ordine viabilistico stabilito."@it ,
                           "Links an ordinance to the established type of traffic order."@en ;
              rdfs:label "stabilisce"@en ,
                         "stabilisce"@it .


###  https://example.com/ordinanza#relativeToTimeEvent
ex:relativeToTimeEvent rdf:type owl:ObjectProperty ;
                       rdfs:domain ti:TimeInterval ;
                       rdfs:range dtx:PublicEvent ;
                       rdfs:comment "Relativo all'orario di inizio dell'evento" ;
                       rdfs:label "Relative to Time Event" .


###  https://w3id.org/italia/onto/CLV/hasAddress
clv:hasAddress rdf:type owl:ObjectProperty ;
               rdfs:domain ex:Luogo_Ordine ;
               rdfs:range clv:Address ;
               rdfs:comment "Concept related to hasaddress."@en ,
                            "Concetto relativo a hasaddress."@it ;
               rdfs:label "has current address"@en ,
                          "hasAddress"@en .


###  https://w3id.org/italia/onto/CLV/hasAddressArea
clv:hasAddressArea rdf:type owl:ObjectProperty ;
                   rdfs:domain ex:Luogo_Ordine ;
                   rdfs:range clv:AddressArea ;
                   rdfs:comment "Concept related to hasaddressarea."@en ,
                                "Concetto relativo a hasaddressarea."@it ;
                   rdfs:label "has address area"@en ,
                              "hasAddressArea"@en .


###  https://w3id.org/italia/onto/COV/hasOrganization
cov:hasOrganization rdf:type owl:ObjectProperty ;
                    rdfs:domain ex:Ordinanza ;
                    rdfs:range cov:Organization ;
                    rdfs:comment "The relation that allows to associate an organization with anything." ;
                    rdfs:label "Has Organization"@it .


###  https://w3id.org/italia/onto/COV/hasSupportUnit
cov:hasSupportUnit rdf:type owl:ObjectProperty ;
                   rdfs:domain cov:SupportUnit ;
                   rdfs:range cov:Organization ;
                   rdfs:comment "Concept related to hassupportunit."@en ,
                                "Concetto relativo a hassupportunit."@it ;
                   rdfs:label "has Department"@en ,
                              "hasSupportUnit"@en .


###  https://w3id.org/italia/onto/TI/atTime
ti:atTime rdf:type owl:ObjectProperty ;
          rdfs:domain ex:Ordine ;
          rdfs:range ti:TimeInterval ;
          rdfs:comment "Concept related to attime."@en ,
                       "Concetto relativo a attime."@it ;
          rdfs:label "at Time"@en ,
                     "atTime"@en .


###  https://w3id.org/italia/onto/TI/hasDuration
ti:hasDuration rdf:type owl:ObjectProperty ;
               rdfs:domain dtx:PublicEvent ;
               rdfs:range ti:Duration ;
               rdfs:comment "This property is used to specify the temporal duration of the validity." ;
               rdfs:label "Has duration" .


###  https://w3id.org/italia/onto/TI/hasTemporalEntity
ti:hasTemporalEntity rdf:type owl:ObjectProperty ;
                     rdfs:domain ex:Ordinanza ;
                     rdfs:range ti:TimeInstant ;
                     rdfs:comment "Property that links a time indexed event to a temporal entity" ;
                     rdfs:label "Has Date"@en ,
                                "data"@it .


#################################################################
#    Data properties
#################################################################

###  http://example.com/ordinanza#numeroProtocollo
ex:numeroProtocollo rdf:type owl:DatatypeProperty ;
                    rdfs:domain ex:Ordinanza ;
                    rdfs:range xsd:string ;
                    rdfs:comment "Alphanumeric code assigned by the public registry when the act is recorded."@en ,
                                 "Codice alfanumerico assegnato in sede di protocollazione informatica dell'atto."@it ;
                    rdfs:label "numero di protocollo"@it ,
                               "protocol number"@en .


###  https://w3id.org/italia/onto/TI/startTime
ti:startTime rdf:type owl:DatatypeProperty ;
             rdfs:domain dtx:PublicEvent ;
             rdfs:comment "This property represents the start date of the time interval" .


